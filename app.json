{
  "expo": {
    "name": "Moodify",
    "slug": "moodify",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo/icon-app-logo.png",
    "backgroundColor": "black",
    "scheme": "moodify",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/logo/logo.png",
      "infoPlist": {
        "UIStatusBarHidden": true,
        "UIViewControllerBasedStatusBarAppearance": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo/icon-app-logo.png",
        "backgroundColor": "#FFDFD4"
      },
      "edgeToEdgeEnabled": true,
      "statusBar": {
        "hidden": true
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/logo/icon-app-logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "android": {
            "image": "./assets/logo/icon-app-logo.png",
            "imageWidth": 200,
            "resizeMode": "contain",
            "backgroundColor": "#FFDFD4"
          },
          "ios": {
            "image": "./assets/logo/logo.png",
            "imageWidth": 200,
            "resizeMode": "contain",
            "backgroundColor": "#FFDFD4"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
